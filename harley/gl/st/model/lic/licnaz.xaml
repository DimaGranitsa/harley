<Page x:Class="harley.gl.st.model.lic.licnaz"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:harley.gl.st.model.lic"
      mc:Ignorable="d" 
      d:DesignHeight="800" d:DesignWidth="1300"
      Title="licnaz">

    <Grid Background="#23262B">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="260"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Image Source="{Binding foto}" Width="800" ></Image>
        </Grid>
        <Grid Grid.Row="1">
            <TextBlock Text="Name" Foreground="White" FontSize="50" />
            <Label Content="{Binding Name}" Foreground="White" FontSize="100" HorizontalAlignment="Center" VerticalAlignment="Center"/>
        </Grid>
    </Grid>
</Page>
